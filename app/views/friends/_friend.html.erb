<tr id="<%= dom_id friend %>">
  <td>
    <%= link_to friend.first_name, friend %>
    <%= link_to friend.last_name, friend %>
  </td>

  <td>
    <%= friend.email %>
  </td>

  <td>
    <%= friend.phone %>
  </td>

  <td>
    <%= friend.twitter %>
  </td>

  <td>
  <%= button_to "Delete", friend, method: :delete, data: { confirm: "Really?" }, class: "btn btn-outline-danger btn-sm" %>
  </td>
</tr>
